{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@react-navigation/core": ["./core/src"],
      "@react-navigation/native": ["./native/src"],
      "react-navigation": ["./react-navigation/src"],
      "react-navigation-animated-switch": ["./animated-switch/src"],
      "react-navigation-drawer": ["./drawer/src"],
      "react-navigation-material-bottom-tabs": ["./material-bottom-tabs/src"],
      "react-navigation-stack": ["./stack/src"],
      "react-navigation-tabs": ["./tabs/src"],
    },
    "composite": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": ["esnext", "dom"],
    "module": "esnext",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "noStrictGenericChecks": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext"
  }
}
